  <connectionStrings>
    <add name="ApplicationServices" connectionString="data source=.\SQLEXPRESS;Database=aspnetdb;Trusted_Connection=true" providerName="System.Data.SqlClient" />
    <add name="macheteConnection" connectionString="data source=.\SQLEXPRESS;Database=machete;Trusted_Connection=true;MultipleActiveResultSets=true" providerName="System.Data.SqlClient" />
    <add name="ELMAHlog" connectionString="data source=.\SQLEXPRESS;Database=machetelog;Trusted_Connection=true" providerName="System.Data.SqlClient" />
    <add name="macheteEntities" connectionString="metadata=res://*/DAL.Model1.csdl|res://*/DAL.Model1.ssdl|res://*/DAL.Model1.msl;provider=System.Data.SqlClient;provider connection string=&quot;Data Source=.\SQLEXPRESS;Initial Catalog=machete;Integrated Security=True;MultipleActiveResultSets=True&quot;" providerName="System.Data.EntityClient" />
  </connectionStrings>
  <appSettings>
    <add key="webpages:Version" value="2.0.0.0" />
    <add key="webpages:Enabled" value="false" />
    <add key="PreserveLoginUrl" value="true" />
    <add key="ClientValidationEnabled" value="true" />
    <add key="UnobtrusiveJavaScriptEnabled" value="true" />
